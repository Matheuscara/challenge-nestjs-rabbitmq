#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matheusdiascara/Documents/Developer/testes/challenge-nestjs/node_modules/seek-bzip/bin/node_modules:/Users/matheusdiascara/Documents/Developer/testes/challenge-nestjs/node_modules/seek-bzip/node_modules:/Users/matheusdiascara/Documents/Developer/testes/challenge-nestjs/node_modules:/Users/matheusdiascara/Documents/Developer/testes/node_modules:/Users/matheusdiascara/Documents/Developer/node_modules:/Users/matheusdiascara/Documents/node_modules:/Users/matheusdiascara/node_modules:/Users/node_modules:/node_modules:/Users/matheusdiascara/Documents/Developer/testes/challenge-nestjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/matheusdiascara/Documents/Developer/testes/challenge-nestjs/node_modules/seek-bzip/bin/node_modules:/Users/matheusdiascara/Documents/Developer/testes/challenge-nestjs/node_modules/seek-bzip/node_modules:/Users/matheusdiascara/Documents/Developer/testes/challenge-nestjs/node_modules:/Users/matheusdiascara/Documents/Developer/testes/node_modules:/Users/matheusdiascara/Documents/Developer/node_modules:/Users/matheusdiascara/Documents/node_modules:/Users/matheusdiascara/node_modules:/Users/node_modules:/node_modules:/Users/matheusdiascara/Documents/Developer/testes/challenge-nestjs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../seek-bzip/bin/seek-bunzip" "$@"
else
  exec node  "$basedir/../seek-bzip/bin/seek-bunzip" "$@"
fi
